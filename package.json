    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "start": "nuxt-ts start",
    "test": "jest --passWithNoTests && npm run link:check"
  },
  "license": "MIT",
  "keywords": [
    "tailwindcss",
    "vue",
    "nuxt",
    "components",
    "ui framework",
    "component framework",
    "ui library",
    "component library"
