{
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt",
    "export": "nuxt export",
    "generate": "nuxt generate",
    "link:mijin": "pnpm link ../mijin && sed -i \"\" \"s|'mijin'|'mijin/src'|\" src/plugins/mijin.ts",
    "link:mijin:remove": "pnpm unlink mijin && pnpm install mijin --save && sed -i \"\" \"s|'mijin/src'|'mijin'|\" src/plugins/mijin.ts",
    "lint": "pnpm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore src",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "start": "nuxt start",
    "test": "jest --passWithNoTests"
  },
  "license": "MIT",
  "keywords": [
    "tailwindcss",
    "vue",
    "nuxt",
    "components",
    "ui framework",
    "component framework",
    "ui library",
    "component library"
  ],
  "config": {
    "nuxt": {
      "host": "0.0.0.0"
    }
  },
  "workspaces": [
    "src/*"
  ],
  "dependencies": {
    "highlight.js": "^10.7.2",
    "mijin": "^1.9.1",
    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.28.1",
    "vue-clipboard2": "^0.3.3"
  },
  "devDependencies": {
    "@babel/core": ">=7.8.0 <8.0.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/color-mode": "^2.1.1",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/svg": "^0.2.0",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@vue/test-utils": "^1.2.2",
    "autoprefixer": "^10.3.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.1",
    "consola": "^2.15.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-nuxt": "^1.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-nuxt": "^2.0.0",
    "jest": "^27.2.1",
    "postcss": "^8.3.7",
    "stylelint": "^13.13.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "tailwindcss": "^3.0.13",
    "ts-jest": "^27.0.5",
    "typescript": "^4.5.4",
    "vue": "^2.6.14",
    "vue-form": "^4.10.3",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  }
}
